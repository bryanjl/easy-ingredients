{
    "name": "Easy Ingredients",
    "description": "Get the ingredients from a recipe page",
    "version": "0.1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup":  "./src/html/popup.html"
    },
    "options_page": "./src/html/options.html",
    "permissions": [    
        "activeTab",
        "tabs",
        "storage",
        "scripting",
        "webNavigation"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "icons": {
        "16": "./img/lemon-icon-16.png",
        "48": "./img/lemon-icon-48.png",
        "128": "./img/lemon-icon-48.png"
    }
}